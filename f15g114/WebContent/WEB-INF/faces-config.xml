</navigation-case>
<navigation-case>
<from-outcome>fail</from-outcome>
<to-view-id>/assessment.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/classStatistics.jsp</from-view-id>
<navigation-case>
<from-outcome>barChart</from-outcome>
<to-view-id>/classStatistics.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>timeSeries</from-outcome>
<to-view-id>/timeSeries.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStatsLoad</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStats</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/descriptiveStats.jsp</from-view-id>
<navigation-case>
<from-outcome>barChart</from-outcome>
<to-view-id>/classStatistics.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>timeSeries</from-outcome>
<to-view-id>/timeSeries.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStatsLoad</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStats</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/timeSeries.jsp</from-view-id>
<navigation-case>
<from-outcome>barChart</from-outcome>
<to-view-id>/classStatistics.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>timeSeries</from-outcome>
<to-view-id>/timeSeries.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStatsLoad</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>descriptiveStats</from-outcome>
<to-view-id>/descriptiveStats.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/viewAssessmentRoster.jsp</from-view-id>
<navigation-case>
<from-outcome>studentList</from-outcome>
<to-view-id>/viewAssessmentRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>studentAssessment</from-outcome>
<to-view-id>/displayStudentAssessment.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/displayStudentAssessment.jsp</from-view-id>
<navigation-case>
<from-outcome>reset</from-outcome>
<to-view-id>/viewAssessmentRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>saveComments</from-outcome>
<to-view-id>/displayStudentAssessment.jsp</to-view-id>
</navigation-case>
</navigation-rule> 

<navigation-rule>  
<from-view-id>/createTable.jsp</from-view-id>
<navigation-case>
<from-outcome>displayTable</from-outcome>
<to-view-id>/dynamicTables.jsp</to-view-id>
</navigation-case>
</navigation-rule> 

<navigation-rule>  
<from-view-id>/dynamicTables.jsp</from-view-id>
<navigation-case>
<from-outcome>clearedList</from-outcome>
<to-view-id>/createTable.jsp</to-view-id>
</navigation-case>
</navigation-rule> 

<managed-bean>
<managed-bean-name>login</managed-bean-name>
<managed-bean-class>edu.uic.ids.model.LoginBean
</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
<managed-bean-name>loginDao</managed-bean-name>
<managed-bean-class>edu.uic.ids.model.LoginDao
</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
<managed-bean-name>professorBean</managed-bean-name>
<managed-bean-class>edu.uic.ids.model.ProfessorBean
</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
<managed-bean-name>chartBean</managed-bean-name>
<managed-bean-class>edu.uic.ids.model.ChartBean
</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
<managed-bean-name>dataValuesBean</managed-bean-name>
<managed-bean-class>edu.uic.ids.DbConnection.DataValuesBean
</managed-bean-class>
<managed-bean-scope>session</managed-bean-scope>
<managed-property>
<property-name>userName</property-name>
<property-class>java.lang.String</property-class>
<value>f15g114</value>
</managed-property>
<managed-property>
<property-name>schema</property-name>
<property-class>java.lang.String</property-class>
<value>f15g114</value>
</managed-property>
<managed-property>
<property-name>dbmsHost</property-name>
<property-class>java.lang.String</property-class>
<value>localhost</value>
</managed-property>

</managed-bean>

</faces-config>
