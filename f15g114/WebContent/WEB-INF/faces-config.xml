<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
<navigation-rule>  
<from-view-id>/userLogin.jsp</from-view-id>
<navigation-case>
<from-outcome>student</from-outcome>
<to-view-id>/studentHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>instructor</from-outcome>
<to-view-id>/instructorHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>tutor</from-outcome>
<to-view-id>/tutorHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>registerUser</from-outcome>
<to-view-id>/newUser.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>error</from-outcome>
<to-view-id>/userLogin.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>createTable</from-outcome>
<to-view-id>/createTable.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/login.jsp</from-view-id>
<navigation-case>
<from-outcome>success</from-outcome>
<to-view-id>/userLogin.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>fail</from-outcome>
<to-view-id>/login.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/instructorHome.jsp</from-view-id>
<navigation-case>
<from-outcome>viewAssessments</from-outcome>
<to-view-id>/viewAssessments.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>viewRoster</from-outcome>
<to-view-id>/viewRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>classGrades</from-outcome>
<to-view-id>/classGrades.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>fail</from-outcome>
<to-view-id>/instructorHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>viewAssessmentRoster</from-outcome>
<to-view-id>/viewAssessmentRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>studentView</from-outcome>
