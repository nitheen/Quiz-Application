</navigation-case>
<navigation-case>
<from-outcome>barChart</from-outcome>
<to-view-id>/classStatistics.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>noCharts</from-outcome>
<to-view-id>/classStatistics.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/tutorHome.jsp</from-view-id>
<navigation-case>
<from-outcome>viewAssessments</from-outcome>
<to-view-id>/viewAssessments.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>viewRoster</from-outcome>
<to-view-id>/viewRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>classGrades</from-outcome>
<to-view-id>/classGrades.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>fail</from-outcome>
<to-view-id>/instructorHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>viewAssessmentRoster</from-outcome>
<to-view-id>/viewAssessmentRoster.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>studentView</from-outcome>
<to-view-id>/studentHome.jsp</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>  
<from-view-id>/studentHome.jsp</from-view-id>
<navigation-case>
<from-outcome>assessmentsPage</from-outcome>
<to-view-id>/assessment.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>viewGrades</from-outcome>
<to-view-id>/viewGrades.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>displayGrades</from-outcome>
<to-view-id>/viewGrades.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>instructor</from-outcome>
<to-view-id>/instructorHome.jsp</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>tutor</from-outcome>
<to-view-id>/tutorHome.jsp</to-view-id>
</navigation-case>
